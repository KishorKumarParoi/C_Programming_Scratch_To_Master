#include <stdio.h>

int main(void)
{
    // printf("Hello World\n");
    // Let's break an infinity Loop

    int i = 0;
    while (1)
    {
        i++;
        if (i == 9)
            continue;
        printf("Hey I am going on and on\n");
        if (i >= 10)
        {
            break;
        }
    }
    printf("You Broke my Heart by printing only 9 times !!!\n");

    return 0;
}